                <span class="badge badge-info"><i class="fas fa-key"></i> Llaveros</span>
                <span class="badge badge-light"><i class="fas fa-coffee"></i> Café</span>
                <span class="badge badge-dark"><i class="fas fa-coffee"></i> Té</span>

            </div>
        </div>
        <div class="container" id="regalosproduct">
            <div class="row">
                <div class="col-md-12">
                        <div class="tab-content" id="myTabContent">

                            <div class="tab-pane fade show active" id="termo" role="tabpanel" aria-labelledby="termo-tab">
                                
                                <div class="container">
                                    <div class="row">
                                        <!--INICIA LA CONSULTA DE PRODUCTOS-->
                                        <% productos.forEach(producto => { %>
                                        <% if(producto.nameCategory == "Foto Regalos"){ %>
                                        <div class="col-md-3">
                                            <div class="card mb-5" style="width: auto">
                                                <img class="card-img-top" src="img/<%= producto.imageProduct %>" alt="Card image cap">
                                                <div class="card-body text-center">
                                                    <h5 class="card-title">
                                                        <font size="3"><%= producto.nameProduct %> uPrint®</font>
                                                        <footer class="blockquote-footer text-center">desde <cite title="Source Title">$<%= producto.priceProduct %> MXN</cite></footer>
                                                    </h5>
                                                    <h5 class="mb-0">
                                                        <div class="showInfo" href="#collapse<%= producto.idProduct %>" data-toggle="collapse" data-parent="#accordion">
                                                            <button type="button" class="btn btn-outline-light" data-toggle="tooltip" data-placement="right" title="Haz click para saber más acerca del producto">
                                                                <h1 class="text-info lead">Información <i class="fas fa-arrow-circle-down"></i></h1>
                                                            </button> 
                                                        </div>
                                                    </h5>
                                                        <div id="collapse<%= producto.idProduct %>" class="collapse">
                                                            <div class="card-body">
                                                                <p class="text-left">
                                                                    <font size="2">
                                                                        <ul class="list-unstyled" style="text-align: left">
                                                                            <ul>
                                                                                    <li class="list-group-item"><span style="font-weight: bold; color: #17a2b8">Detalles del producto:</span><%= producto.featuresProduct %></li>
                                                                                    <li class="list-group-item"><span style="font-weight: bold; color: #17a2b8;">Dimensiones:</span><%= producto.widthDimension %> X <%= producto.heightDimension %></li>
                                                                                    <li class="list-group-item"><span style="font-weight: bold; color: #17a2b8;">Pedidos maximos:</span><%= producto.max %></li>
                                                                                    <li class="list-group-item"><span style="font-weight: bold; color: #17a2b8;">Pedidos minimos:</span><%= producto.min %></li>
                                                                                    <li class="list-group-item"><span style="font-weight: bold; color: #17a2b8;">Tiempo de entrega:</span><%= producto.time %></li>
                                                                                    <% if(producto.enableProduct == 1){ %>
                                                                                    <li class="list-group-item"><span style="font-weight: bold; color: #17a2b8;">Existencia en almacen:</span> <span style="color: rgb(41, 138, 83);" >Si</span></li>
                                                                                    <% } else{ %>  
                                                                                    <li class="list-group-item"><span style="font-weight: bold; color: #17a2b8; ">Existencia en almacen:</span><span style="color: rgb(177, 40, 40);">No</span></li>        
                                                                                    <% } %>  
                                                                            </ul>
                                                                        </ul>
                                                                    </font>
                                                                </p>
                                                            </div>
                                                        </div>
                                                         <a href="#" class="btn btn-info disabled" role="button" aria-disabled="true">Comprar (no disponible)</a>
                                                    </div>
                                                </div>
                                            </div>
                                        
                                        <% } }); %>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
    </section>
</section>
    
